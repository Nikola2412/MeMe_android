<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.meme.ui.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upload"
        android:name="com.example.meme.FragmentUpload"
        android:label="@string/app_name"
        />

    <fragment
        android:id="@+id/navigation_meme"
        android:name="com.example.meme.MemesFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_memes"
        />

    <fragment
        android:id="@+id/videos"
        android:name="com.example.meme.playerFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_player"/>
</navigation>